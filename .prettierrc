{
  "singleQuote": true,
  "printWidth": 80,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react",
    "^next",
    "<THIRD_PARTY_MODULES>",
    "^@/env/(.*)$",
    "^@/firebase/(.*)$",
    "^@/components/(.*)$",
    "^@/utils/(.*)$",
    "^@/contents/(.*)$",
    "^@/styles/(.*)$",
    "^@/types/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
